{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "src/index.ts": {
      "runtime": "vercel-bun@latest",
      "includeFiles": "public/**/*"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "src/index.ts"
    }
  ]
}